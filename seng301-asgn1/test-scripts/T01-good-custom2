CREATE(5, 10, 25, 100; 3)
CONFIGURE([0] "Coke", 100; "water", 250; "stuff", 205)
COIN_LOAD([0] 0; 5, 2)
COIN_LOAD([0] 1; 10, 1)
COIN_LOAD([0] 2; 25, 2)
POP_LOAD([0] 0; "Coke", 1)
POP_LOAD([0] 1; "water", 1)
POP_LOAD([0] 2; "stuff", 2)
PRESS([0] 0)
EXTRACT([0])
CHECK_DELIVERY(0)
INSERT([0] 100)
INSERT([0] 10)
PRESS([0] 0)
EXTRACT([0])
CHECK_DELIVERY(10,"Coke")
UNLOAD([0])
CHECK_TEARDOWN(60; 110; "water", "stuff", "stuff")
